<ellipse
    style="fill:none;stroke:#000000;stroke-width:1"
    id="geometry{{ geometry.id }}"
    {%
        include "@hc/blueprint/data.svg.twig" with {
        'modules': modules,
        'blueprintId': geometry.blueprintId,
        'geometryId': geometry.id,
    }
    %}
    cx="{{ offsetLeft+geometry.left+(geometry.width/2) }}"
    cy="{{ offsetTop+geometry.top+(geometry.height/2) }}"
    rx="{{ geometry.width/2 }}"
    ry="{{ geometry.height/2 }}"
/>